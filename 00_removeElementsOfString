//remove the elements from the string with and without the replace method

//with replace method
let string = "I am learing the javascript code"
let string2 = string.replace("the","")
console.log(string2);

//without replace method
(function(string) {
    let array = string.split(" ")
    let arrayRemove = array.splice(2,4);
    console.log(arrayRemove);
})("I am learing the javascript code")

